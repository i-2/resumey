        "@emotion/react": "^11.1.4",
        "@emotion/styled": "^11.0.0",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/jest": "^24.0.0",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.56",
        "@types/react-dom": "^16.9.9",
        "@types/react-redux": "^7.1.11",

        "buildprod": "NODE_ENV=production parcel watch frontend/index.tsx --out-dir static/js",
        "boom": "NODE_ENV=production parcel build frontend/index.tsx --out-dir static/js --no-source-maps --detailed-report --log-level 0"
    },
    "repository": {
        "type": "git",

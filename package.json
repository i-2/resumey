        "react-dom": "^16.13.1",
        "react-redux": "^7.2.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.3",
        "react-star-rating-component": "^1.4.1",
        "react-test-renderer": "^17.0.1",
        "redux": "^4.0.5",
        "redux-saga": "^1.1.3",
        "typescript": "~3.7.2",
        "yup": "^0.32.8"

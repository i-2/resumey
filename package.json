    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-syntax-jsx": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.12.7",
        "@types/jest": "^26.0.19",
        "@types/mocha": "^8.2.0",
        "babel-jest": "^26.6.3",
        "jest": "^26.6.3",
        "parcel": "^1.12.4",
        "redux-devtools": "^3.7.0",
        "redux-devtools-dock-monitor": "^1.2.0",
